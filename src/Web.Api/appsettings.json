{
  "ConnectionStrings": {
    "PersistenceConnectionForMysqlDb": "Server=localhost;Port=3306;Database=DbName;Uid=root;Pwd=admin;"
    
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "restrictedToMinimumLevel": "Information"
      },
      {
        "Name": "File",
        "Args": {
          "path": "serilogs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Properties": {
      "Application": "Tax"
    }
  },
  "AllowedHosts": "*",
  "urls": "http://localhost:5020"
  //"urls": "http://localhost:8020"
}